# mount point       fstype    device                                                                flags
/boot               emmc      boot                                                                  flags=display="Boot";backup=1;flashimg=1;logical;
/cache              ext4      /dev/block/mmcblk0p2                                                  flags=display="Cache";
/data               ext4      /dev/block/mmcblk0p3                                                  flags=display="Data";
/misc               emmc      misc                                                                  flags=display="Misc";backup=1;flashimg=1;logical;
/recovery           emmc      recovery                                                              flags=display="Recovery";backup=1;flashimg=1;logical;
/sdcard1            vfat      /dev/block/mmcblk0p4                                                  flags=display="Sdcard1";
/system             ext4      /dev/block/mmcblk0p6                                                  flags=display="System";backup=1;
/system_image       emmc      /dev/block/mmcblk0p6                                                  flags=display="System image";backup=1;flashimg=1;
